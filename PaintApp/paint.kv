#:kivy 1.0.9

<PaintWidget>:
    size_hint: None, None
    size: 10, 10
    canvas:
        Ellipse:
            pos: self.pos
            size: self.size
  
<Canvas>:
    canvas:
        Color:
            rgb: 255, 255, 255, 1
        Rectangle:
            size: root.size
            pos: root.pos
